package keresztezodes.x.model;

class aLampa {
	
	initial Init;

	state Piros;

	state PirosSarga;

	state Zold;

	state Sarga;


	transition Initialize {
		from Init;
		to Piros;
	}
	
	transition PtoPS {
		from Piros;
		to PirosSarga;
		trigger Lepes;
	}
	
		transition PStoZ {
		from PirosSarga;
		to Zold;
		trigger Lepes;
	}
	
		transition ZtoS {
		from Zold;
		to Sarga;
		trigger Lepes;
	}
	
		transition StoP {
		from Sarga;
		to Piros;
		trigger Lepes;
	}
	
	
}
