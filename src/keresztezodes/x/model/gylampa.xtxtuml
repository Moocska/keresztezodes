package keresztezodes.x.model;

class gyLampa {
	
	initial Init;

	state Zold;

	state ZVillog;

	state Piros;


	transition Initialize {
		from Init;
		to Zold;
	}
	
	transition ZtoZV {
		from Zold;
		to ZVillog;
		trigger Lepes;
	}
	
		transition ZVtoP {
		from ZVillog;
		to Piros;
		trigger Lepes;
	}
	
	
		transition PtoZ {
		from Piros;
		to Zold;
		trigger Lepes;
	}
	
	
}
